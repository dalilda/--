    //     "command": "redo",
    //     "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    // },  
    
    // {
    //     "key": "alt+.",
    //     "command": "cursorRight",
    //     "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    // },
    {
        "key": "alt+;",
        "command": "cursorRight",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "alt+.",
        "command": "cursorEnd",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    
